generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BlogPost {
  id          String   @id @default(uuid())
  title       String
  content     String
  imageurl    String
  authorid    String
  authorname  String
  authorimage String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
